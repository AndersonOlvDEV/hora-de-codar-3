programa
{
    funcao inicio()
    {
        // Declara a variável que será usada como contador no laço.
        inteiro numero

        escreval("Os 10 primeiros números inteiros maiores que 100 são:")

        // O laço 'para' começa com o primeiro número maior que 100, que é 101,
        // e vai até o décimo número, que é 110.
        para numero de 101 ate 110 faca
            escreval(numero)
        fimpara
    }
}
